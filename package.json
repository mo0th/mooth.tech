{
  "name": "soorria.com",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint --fix src",
    "format": "run-s format:*",
