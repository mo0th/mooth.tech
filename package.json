    "husky:install": "husky install",
    "prepare": "is-ci-cli \"\" \"husky:install\""
  },
  "dependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@mdx-js/mdx": "^2.1.3",
    "@next/bundle-analyzer": "^10.2.0",
    "@supabase/supabase-js": "^1.27.0",
    "esbuild": "^0.14.48",
    "globby": "^11.0.3",
    "goober": "^2.1.9",
    "gray-matter": "^4.0.2",
    "hast": "^1.0.0",
    "mdast": "^3.0.0",
    "mdx-bundler": "^9.0.1",
    "mdx-prism": "^0.3.3",
    "mitt": "^3.0.0",
    "next": "^12.2.0",
    "next-plausible": "^3.1.3",
    "next-seo": "^4.23.0",
    "next-sitemap": "^3.1.22",
    "prettier": "^2.7.1",
